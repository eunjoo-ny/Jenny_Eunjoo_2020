    "def factorial(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "print(\"1!:\",factorial(1))\n",
    "print(\"2!:\",factorial(2))\n",
    "print(\"3!:\",factorial(3))\n",
    "print(\"4!:\",factorial(4))\n",
    "print(\"5!:\",factorial(5))"
